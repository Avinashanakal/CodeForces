#include <bits/stdc++.h>
using namespace std;

int main() {
	int t,n;
	cin>>t;
	while(t--){
	    int n;
		cin>>n;
		int arr[n];
		for(int i=0;i<n;++i){
		    cin>>arr[i];
		}
		sort(arr,arr+n);
		int res=arr[n-1]-arr[0];
		for(int i=0;i<n;++i){
		    for(int j=i+1;j<n;++j)
		    res=min(res,arr[j]-arr[i]);
		}
		cout<<res<<"\n";
	}
	return 0;
}
