#include <bits/stdc++.h>
#define lli long long int
using namespace std;

int main() {
    string g,h,p;
    cin>>g>>h>>p;
    
    string ans=g+h;
    
    sort(ans.begin(),ans.end());
    sort(p.begin(),p.end());
    
    if(ans==p)
    cout<<"YES"<<endl;
    
    else
    cout<<"NO"<<endl;
    
	return 0;
}
